{
	"manifest_version": 3,
	"name": "Headspace",
	"description": "This extension renders the contents of <head> element visible.",
	"version": "1.2",
	"icons": {
		"16": "images/icon-16.png",
		"32": "images/icon-32.png",
		"48": "images/icon-48.png",
		"128": "images/icon-128.png"
	},
	"background": {
		"service_worker": "background.js"
	},
	"permissions": [
		"activeTab",
		"scripting"
	],
	"action": {
		"default_icon": {
			"16": "images/icon-16.png",
			"32": "images/icon-32.png",
			"48": "images/icon-48.png",
			"128": "images/icon-128.png"
		}
	}
}